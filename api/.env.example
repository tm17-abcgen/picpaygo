# OpenRouter AI
OPENROUTER_API_KEY=sk-or-v1-...
OPENROUTER_API_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_MODEL=google/gemini-3-pro-image-preview

# Worker config
JOB_WORKERS=2
JOB_TIMEOUT_SECONDS=120

# CORS
CORS_ORIGINS=http://localhost:8082

# Session
SESSION_TTL_HOURS=168
FREE_CREDITS=3

# MinIO (object storage)
# Images are served via the API proxy endpoint (/api/images/{bucket}/{key})
# No special DNS configuration required.
MINIO_ENDPOINT=minio:9000
MINIO_USE_HTTPS=false

# Stripe (payment processing)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Frontend URL (for Stripe success/cancel redirects)
FRONTEND_URL=http://localhost:8080

# Price IDs (format: {dollars}_{credits} - price_id)
STRIPE_PRICE_ID_2_5=price_...
STRIPE_PRICE_ID_3_10=price_...
STRIPE_PRICE_ID_5_20=price_...
STRIPE_PRODUCT_ID_2_5=prod_...
STRIPE_PRODUCT_ID_3_10=prod_...
STRIPE_PRODUCT_ID_5_20=prod_...
